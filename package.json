{
  "name": "seafood-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:cms": "npm --workspace @seafood/cms run dev",
    "dev:web": "npm --workspace @seafood/web run dev",
    "build": "npm --workspace @seafood/web run build && npm --workspace @seafood/cms run build",
    "lint": "echo \"(add your linter here)\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "react": "^19.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "vite-plugin-pages": "^0.33.1"
  }
}
